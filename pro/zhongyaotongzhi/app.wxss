@import "tmp/tmp.wxss";

page {
    height: 100%;
    background-color: #efeff4;
    font-family: "Helvetica Neue",sans-serif;
}

button,button::after,.button-hover {
    margin-left: 0;
    margin-right: 0;
    padding-left: 0;
    padding-right: 0;
    box-sizing: unset;
    border: 0;
    border-radius: 0;
    background-color: transparent;
    text-align: inherit;
    line-height: 1;
    -webkit-tap-highlight-color: transparent;
}

.button {
    width: 690rpx;
    height: 86rpx;
    background-color: #1677d2;
    color: #fff;
    font-size: 36rpx;
    margin-bottom: 20rpx;
    position: relative;
    display: block;
    margin-left: auto;
    margin-right: auto;
    padding-left: 28rpx;
    padding-right: 28rpx;
    box-sizing: border-box;
    text-align: center;
    text-decoration: none;
    line-height: 2.55555556;
    border-radius: 10rpx;
    -webkit-tap-highlight-color: transparent;
    overflow: hidden;
}

.button.light {
    background-color: #a0e2fb;
    color: #e5f7fe;
}

.button.gray {
    background-color: #dfdfdf;
    color: #b0b0b0;
}

.ads1 {
    padding-top: 30rpx;
    text-align: center;
    color: #666;
    font-size: 26rpx;
}

.ads2 {
    padding: 10rpx 0 50rpx 0;
    text-align: center;
    color: #b2b2b2;
    font-size: 24rpx;
}

.notice-footer {
    width: 100%;
    height: 100rpx;
    position: fixed;
    bottom: 0;
    display: flex;
    flex-direction: row;
    align-items: center;
    background-color: #fff;
    z-index: 1;
}

.notice-footer-divider {
    width: 1rpx;
    height: 100%;
    background-color: #eee;
}

.notice-footer-item {
    position: relative;
    width: 180rpx;
    height: 99rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    border-top: 1rpx solid #eee;
}

.notice-footer-item-icon {
    width: 36rpx;
    height: 36rpx;
}

.notice-footer-item-text {
    padding-top: 10rpx;
    color: #666;
    font-size: 24rpx;
}

.notice-footer-item .contact {
    position: absolute;
    left: 0;
    top: 0;
    z-index: 10;
    height: 100%;
    opacity: 0;
}

.notice-footer-button {
    height: 100%;
    flex-grow: 1;
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: center;
    background-color: #1677d2;
    color: #fff;
    font-size: 36rpx;
}

.error {
    display: flex;
    flex-direction: column;
    align-items: center;
    padding: 150rpx 30rpx 0 30rpx;
    color: #666;
    font-size: 30rpx;
}

.error-icon {
    width: 140rpx;
    height: 140rpx;
    margin-bottom: 30rpx;
}

.error-text {
    margin-bottom: 20rpx;
}

.error-refresh {
    padding: 15rpx 20rpx;
    border: 1rpx solid #ccc;
    color: #999;
    font-size: 30rpx;
}

.dialog,.d-mask {
    position: fixed;
    left: 0;
    top: 0;
    right: 0;
    bottom: 0;
    z-index: 100;
}

.d-mask {
    z-index: 101;
    background: rgba(0,0,0,.4);
}

.d-content {
    position: fixed;
    left: 50%;
    top: 50%;
    z-index: 102;
    transform: translate(-50%,-50%);
    width: 600rpx;
    background: #fff;
    border-radius: 10rpx;
}

.d-header {
    text-align: center;
    font-size: 34rpx;
    color: #040404;
    font-weight: 900;
    padding: 30rpx 20rpx 0;
}

.d-body {
    padding: 20rpx 30rpx;
    color: #999;
    font-size: 26rpx;
    line-height: 1.5;
}

.d-body .blue {
    color: #1577d2;
}

.d-footer {
    text-align: center;
    color: #1577d2;
    line-height: 2.8;
    font-size: 34rpx;
    border-top: 1px solid #ccc;
}

.body-height {
    max-height: 360rpx;
}

.unauthorized {
    position: fixed;
    left: 0;
    top: 0;
    right: 0;
    bottom: 0;
    z-index: 120;
    background: #fff;
}

.unauth-img {
    display: block;
    width: 110rpx;
    height: 120rpx;
    margin: 200rpx auto 30rpx;
}

.unauth-title {
    font-size: 30rpx;
    color: #333;
    text-align: center;
    margin-bottom: 20rpx;
}

.unauth-desc {
    font-size: 26rpx;
    color: #999;
    line-height: 1.8;
    text-align: center;
}

.unauth-btn {
    width: 240rpx;
    line-height: 80rpx;
    height: 80rpx;
    color: #fff;
    background: #12b7f5;
    text-align: center;
    margin: 30rpx auto 0;
    font-size: 30rpx;
}

.f-text {
    font-size: 24rpx;
    padding-top: 20rpx;
    text-align: center;
    color: #aaa;
}

.modal-image-text,.modal-mask {
    position: fixed;
    left: 0;
    top: 0;
    right: 0;
    bottom: 0;
    z-index: 15;
}

.modal-mask {
    background: rgba(0,0,0,.5);
}

.modal-content {
    position: fixed;
    left: 50%;
    top: 50%;
    z-index: 16;
    background: #fff;
    width: 560rpx;
    transform: translate(-50%,-50%);
    border-radius: 15rpx;
}

.modal-header {
    text-align: center;
    font-size: 34rpx;
    font-weight: 900;
    line-height: 2.5;
    padding-top: 30rpx;
}

.modal-body {
    padding: 0 30rpx 30rpx;
    font-size: 28rpx;
    line-height: 1.5;
    color: #999;
}

.modal-body image {
    display: block;
    width: 100%;
    margin: 30rpx 0 20rpx;
}

.modal-footer {
    overflow: hidden;
    border-top: 1px solid #eee;
}

.modal-btn {
    float: left;
    width: 100%;
    overflow: hidden;
    text-align: center;
    font-size: 34rpx;
    line-height: 3;
    color: #999;
}

.modal-btn.width50 {
    width: 50%;
}

.modal-ok {
    position: relative;
    color: #1577d2;
}

.modal-ok::before {
    content: "";
    border-left: 1px solid #eee;
    position: absolute;
    left: 0;
    top: 0;
    height: 100%;
}

.dialog,.mask {
    position: fixed;
    left: 0;
    top: 0;
    right: 0;
    bottom: 0;
    z-index: 1000;
}

.mask {
    background: rgba(0,0,0,.4);
}

.dialog-body {
    position: fixed;
    left: 50%;
    top: 50%;
    transform: translate(-50%,-50%);
    background: #fff;
    border-radius: 10rpx;
    z-index: 1001;
}

.dialog-close {
    position: absolute;
    right: 0;
    top: 0;
}

.swiper,.slide-image {
    width: 750rpx;
    height: 214rpx;
}

.swiper-radius,.swiper-radius .slide-image,ad {
    margin: 0 auto;
    width: 690rpx;
    height: 196rpx;
}

.swiper-radius swiper-item {
    border-radius: 10rpx;
}

.ad-bottom {
    padding-bottom: 120rpx;
}