.free-get-list.fixed {
    overflow: hidden;
    position: fixed;
    width: 100%;
}

.free-get-header {
    position: relative;
    font-size: 0;
    overflow: hidden;
}

.head_bg {
    width: 100%;
}

.check-rule-btn {
    width: 100rpx;
    height: 54rpx;
    border: none;
    font-size: 0;
    position: absolute;
    right: -20rpx;
    top: 0;
    padding: 20rpx;
    overflow: hidden;
}

.check-rule-btn image {
    width: 100%;
    height: 100%;
}

.custom-tabbar {
    position: fixed;
    bottom: 0;
    left: 0;
    width: 100%;
    height: 100rpx;
    background: #f7f7f7;
    box-shadow: 0 -1rpx 15rpx rgba(0,0,0,0.1);
    display: flex;
    justify-content: space-around;
    align-items: center;
}

.custom-tabbar .gap-line {
    height: 52rpx;
    background: #ddd;
    width: 2rpx;
}

.custom-tabbar .tabbar-item {
    width: 50%;
    flex: 1;
    text-align: center;
    font-size: 36rpx;
    height: 100%;
    display: flex;
    align-items: center;
    justify-content: center;
    color: #666;
    font-family: PingFangHK-Regular;
}

.custom-tabbar .tabbar-item.active {
    color: #f86642;
}

.no-data {
    display: flex;
    flex-direction: column;
    align-items: center;
}

.no-data image {
    width: 292rpx;
    height: 302.8rpx;
    opacity: 0.5;
    margin: 114rpx auto 0 auto;
}

.no-data .no-data-txt {
    font-size: 28rpx;
    color: #9d9d9d;
    margin-top: 46rpx;
    line-height: 1.5;
}

.no-data button {
    width: 500rpx;
    height: 80rpx;
    border-radius: 200rpx;
    background: #ffece8;
    font-size: 36rpx;
    color: #f86442;
    letter-spacing: 0.47px;
    margin-top: 102rpx;
    line-height: 80rpx;
}

.rules-box-main {
    padding: 44rpx 40rpx 0rpx 40rpx;
}

.rules-tit {
    font-size: 44rpx;
    color: #212637;
    margin-bottom: 24rpx;
    text-align: center;
}

.rules {
    flex: 1;
    overflow: hidden;
}

.rule {
    font-size: 28rpx;
    font-family: PingFang-SC-Medium;
    font-weight: 300;
    color: #787b85;
    text-align: left;
    margin-bottom: 30rpx;
}

.rule .strong {
    color: #f86442;
}

.app-box {
    position: fixed;
    left: 0;
    right: 0;
    top: 0;
    bottom: 0;
    overflow: hidden;
    -webkit-overflow-scrolling: touch;
    text-align: center;
    z-index: 9;
    width: 100vw;
    height: 100vh;
}

.app-box:before {
    content: '';
    position: absolute;
    left: 0;
    right: 0;
    top: 0;
    bottom: 0;
    background: rgba(0,0,0,0.7);
    z-index: -1;
}

.app-box:after {
    content: '';
    display: inline-block;
    width: 0;
    height: 100%;
    overflow: hidden;
    vertical-align: middle;
}

.app-box-main {
    width: 640rpx;
    height: 780rpx;
    max-width: 99%;
    background: #fff;
    border-radius: 24rpx;
    overflow: hidden;
    box-sizing: border-box;
    margin: 192rpx auto auto auto;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
}

.app-box-pic-wrap {
    width: 452rpx;
    margin: 120rpx auto 40rpx;
}

.app-box-pic {
    max-width: 100%;
}

.app-box-text {
    font-size: 14px;
    color: #9d9d9d;
    letter-spacing: 0;
    line-height: 22px;
    text-align: center;
    margin-bottom: 110rpx;
}

.app-box-text text {
    display: block;
}

button.btn-know {
    display: block;
    padding: 0;
    border: none;
    font-size: 36rpx;
    color: #ff744b;
    width: 100%;
    text-align: center;
    height: 110rpx;
    line-height: 110rpx;
}

.btn-know-area {
    border-top: 1px solid #ccc;
}