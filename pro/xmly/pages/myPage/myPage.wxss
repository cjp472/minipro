.login-yes .my-page {
    display: block;
}

.login-yes .authorization {
    display: none;
}

.login-no .my-page {
    display: none;
}

.login-no .authorization {
    display: flex;
}

.login-default .my-page {
    display: none;
}

.login-default .authorization {
    display: none;
}

.my-page {
    width: 100%;
    position: relative;
}

.user-info {
    display: flex;
    align-items: center;
}

.user-info-bg {
    position: absolute;
    left: 0;
    right: 0;
    width: 100%;
    height: 260rpx;
    background-image: linear-gradient(-1deg,#fdae77 0%,#f84242 100%);
}

.user-info-inner {
    overflow: hidden;
    display: flex;
    align-items: center;
    justify-content: space-between;
    width: 100%;
    padding: 30px 0;
    z-index: 10;
    background-color: #f3f4f6;
    background-image: url('data:image/png;base64,');
    background-size: 100% 100%;
    background-repeat: no-repeat;
}

.user-info-inner-left {
    display: flex;
    align-items: center;
    margin-left: 40rpx;
}

.user-avator-outer {
    position: relative;
    overflow: visible;
}

.user-avator {
    flex: 0 0 auto;
    width: 60px;
    height: 60px;
    border-radius: 50%;
    margin-right: 11px;
    background: url('https://img.alicdn.com/imgextra/i4/732450928/TB2vNE2c4xmpuFjSZFNXXXrRXXa_!!732450928.png') no-repeat center;
    background-size: 100%;
}

.user-avator.is-wechat-login::after {
    content: '';
    z-index: 1;
    top: 80rpx;
    left: 80rpx;
    position: absolute;
    width: 38rpx;
    height: 38rpx;
    background-repeat: no-repeat;
    background-size: 100% 100%;
    background-image: url('data:image/png;base64,');
}

.user-box {
    display: flex;
    align-items: center;
}

.text-nick {
    font-size: 36rpx;
    color: #333;
    margin-right: 23rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    font-family: PingFang-SC-Medium;
}

.vip-icon {
    display: inline-block;
    width: 40rpx;
    height: 40rpx;
    background-repeat: no-repeat;
    background-size: 100% 100%;
}

.vip-icon.is-vip {
    background-image: url('data:image/png;base64,');
}

.vip-icon.not-vip {
    background-image: url('data:image/png;base64,');
}

.vip-info-text {
    font-family: PingFang-SC-Medium;
    font-size: 24rpx;
    color: #999;
    margin-top: 10rpx;
}

.exit-wrap {
    display: flex;
    align-items: center;
    margin-right: 40rpx;
    height: 17px;
}

.text-exit {
    display: inline-block;
    opacity: 0.6;
    font-family: PingFangSC-Regular;
    font-size: 28rpx;
    color: #fff;
    margin-right: 16rpx;
}

.exit-wrap .icon-arrow-right {
    width: 20rpx;
    height: 28rpx;
    background: url('https://img.alicdn.com/imgextra/i2/732450928/TB2IZLyb9FmpuFjSZFrXXayOXXa_!!732450928.png') no-repeat center;
    background-size: 100%;
}

.subscription-header {
    display: block;
    font-family: PingFangSC-Regular;
    font-size: 30rpx;
    color: #000;
    margin: 30rpx 30rpx 0;
}

.history-list-item {
    display: flex;
    flex-flow: row;
    padding-left: 30rpx;
    padding-right: 0;
    align-items: flex-start;
}

.history-image {
    flex: 0 0 auto;
    width: 128rpx;
    height: 128rpx;
    background: #ccc;
    margin-right: 30rpx;
    background: url('https://img.alicdn.com/imgextra/i2/732450928/TB22yXCbipnpuFjSZFIXXXh2VXa_!!732450928.png') no-repeat center;
    background-size: 100%;
    margin-top: 30rpx;
    border: 2rpx solid #f1f1f1;
}

.history-info {
    display: flex;
    flex-flow: column;
    flex: 1;
    padding: 30rpx 60rpx 30rpx 0;
    border-bottom: 1rpx solid #efefef;
    position: relative;
    width: 202rpx;
}

.icon-dropdown-down,.icon-dropdown-up {
    display: inline-block;
    width: 64rpx;
    height: 64rpx;
    position: absolute;
    right: 10rpx;
    top: 18rpx;
}

.icon-dropdown-down {
    background: url('https://img.alicdn.com/imgextra/i2/732450928/TB2oFhwdohnpuFjSZFEXXX0PFXa_!!732450928.png') no-repeat center;
    background-size: 100%;
}

.icon-dropdown-up {
    background: url('https://img.alicdn.com/imgextra/i2/732450928/TB2oFhwdohnpuFjSZFEXXX0PFXa_!!732450928.png') no-repeat center;
    background-size: 100%;
}

.history-title-wrap {
    display: flex;
    align-items: center;
    margin-bottom: 10rpx;
}

.tag-paid {
    display: inline-block;
    font-family: PingFangSC-Regular;
    font-size: 22rpx;
    color: #fff;
    background: #e0c69b;
    border-radius: 1px;
    padding: 2rpx 8rpx;
    margin-right: 10rpx;
}

.history-title {
    font-family: PingFangSC-Light;
    flex: 1;
    font-size: 30rpx;
    color: #333;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 1;
    -webkit-box-orient: vertical;
}

.history-intro {
    display: block;
    max-width: 90%;
    font-family: PingFangSC-Light;
    font-size: 24rpx;
    color: #999;
    line-height: 30rpx;
    margin-bottom: 20rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.history-user-wrap {
    display: flex;
    align-items: center;
    justify-content: space-between;
    overflow: visible;
}

.history-user-left {
    display: flex;
    align-items: center;
}

.icon-user {
    width: 20rpx;
    height: 20rpx;
    background: url('https://img.alicdn.com/imgextra/i3/732450928/TB2DIaOe5pnpuFjSZFkXXc4ZpXa_!!732450928.png') no-repeat center;
    background-size: 100%;
    margin-right: 8rpx;
}

.icon-calendar {
    width: 18rpx;
    height: 16rpx;
    background: url('https://img.alicdn.com/imgextra/i2/732450928/TB2oyjld8NkpuFjy0FaXXbRCVXa_!!732450928.png') no-repeat center;
    background-size: 100%;
    margin-right: 8rpx;
}

.text-user {
    font-family: PingFangSC-Light;
    font-size: 22rpx;
    color: #999;
    margin-right: 26rpx;
}

.history-user-right {
    font-family: PingFangSC-Light;
    font-size: 22rpx;
    color: #3f3f3f;
