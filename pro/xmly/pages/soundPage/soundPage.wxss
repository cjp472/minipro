}

.playtimes-wrap {
    display: inline-block;
    padding: 0 6px;
    background-image: linear-gradient(-90deg,rgba(51,51,51,0.00) 0%,rgba(51,51,51,0.35) 100%);
}

button.btn-know {
    display: block;
    padding: 0;
    border: none;
    font-size: 18px;
    color: #fff;
    letter-spacing: 0.47px;
    width: 500rpx;
    height: 40px;
    line-height: 40px;
    border-radius: 20px;
    background: linear-gradient(90deg,#ff9068 0%,#ff6b41 100%);
