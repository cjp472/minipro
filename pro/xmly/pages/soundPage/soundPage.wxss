}

.icon-to-wxq {
    background-image: url("data:image/png;base64,");
}

.authorize-box .app-box-main {
    width: 680rpx;
}

.authorize-box .app-box-pic-wrap {
    font-size: 24px;
    line-height: 33px;
    color: #505050;
    font-weight: bold;
    margin: 30px auto 15px;
}

.authorize-box .app-box-text {
