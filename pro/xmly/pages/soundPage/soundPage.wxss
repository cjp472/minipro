    padding: 26rpx;
    margin-top: 26rpx;
}

.btn-to-pay-wx {
    background: #f86442;
}

.btn-to-pay-xd {
    background: #f6a623;
}

.icon-close-box {
    position: absolute;
    top: 0;
    right: 0;
    width: 80rpx;
    height: 80rpx;
    background-image: linear-gradient(to top right,#fff 0%,#fff 50%,rgba(245,245,245,0.8) 50%,rgba(245,245,245,0.8) 100%);
    color: #d8d8d8;
    font-size: 40rpx;
    text-align: right;
    padding-right: 10rpx;
}

.icon-close-box image {
    width: 40rpx;
    height: 40rpx;
}

.timer {
    font-size: 14px;
}

.sub-count {
    font-size: 12px;
    color: #999;
}

.plus {
    font-size: 14px;
    font-weight: bold;
    padding-right: 4px;
}

.btn-subscribe .plus {
    font-size: 18px;
    line-height: 18px;
    display: inline-block;
    margin-top: -2px;
}

.btn-share image {
    width: 98px;
    height: 30px;
}

.icon-sound-close-time {
    width: 44rpx;
    height: 44rpx;
    display: block;
    background: url("data:image/png;base64,");
    background-repeat: no-repeat;
    background-size: 100%;
