.bg {
    position: fixed;
    left: -50px;
    right: -50px;
    top: -50px;
    bottom: -50px;
    background: center/cover no-repeat;
    background-attachment: fixed;
    overflow: hidden;
    filter: blur(65px) brightness(1) contrast(1) grayscale(0) hue-rotate(0) invert(0.1) opacity(0.15) saturate(1) sepia(0.1);
}

.bg-1 {
    filter: blur(5px);
    opacity: 0.3;
    z-index: 9;
}

.container {
    position: relative;
    z-index: 1;
    background: transparent;
}

.sound-intro {
    display: flex;
    flex-flow: column;
    padding: 0;
}

.sound-title {
    box-sizing: border-box;
    display: flex;
    max-height: 60px;
    margin: 20rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    text-align: center;
}

.sound-title-text {
    vertical-align: middle;
    font-size: 40rpx;
    color: #000;
    line-height: 30px;
    font-weight: bold;
    padding: 0 10px;
}

.user-info {
    display: flex;
    flex-flow: row;
    justify-content: space-between;
    align-items: center;
    border-bottom: 10px solid #f3f4f6;
    padding: 8px;
}

.user-info-left {
    display: flex;
    align-items: center;
    flex: 1;
    margin-right: 24rpx;
}

.user-logo {
    width: 50px;
    height: 50px;
    border-radius: 2px;
    margin-right: 10px;
    background: url('https://img.alicdn.com/imgextra/i1/732450928/TB2yQfacCJjpuFjy0FdXXXmoFXa_!!732450928.png') no-repeat center;
    background-size: 100%;
}

.album-title {
    font-family: PingFangSC-Light;
    font-size: 14px;
    color: #333;
    flex: 1;
}

.btn-subscribe {
    display: flex;
    align-items: center;
    justify-content: center;
    width: 74px;
    height: 28px;
    line-height: 26px;
    background: #ffece8;
    border-radius: 14px;
    font-size: 12px;
    border: 1px solid transparent;
    font-family: PingFangSC-Light;
    color: #f86442;
}

.btn-unsubscribe {
    border: 1px solid #999;
    color: #999;
    background: transparent;
}

.icon-add {
    width: 16rpx;
    height: 16rpx;
    background: url('https://img.alicdn.com/imgextra/i1/732450928/TB2z.VqXHBmpuFjSZFuXXaG_XXa_!!732450928.png') no-repeat center;
    background-size: 100%;
    margin-right: 10rpx;
}

.sound-info {
    display: flex;
    flex-flow: column;
    align-items: center;
    position: relative;
    margin-bottom: 48rpx;
}

.sound-image {
    width: 460rpx;
    height: 460rpx;
    position: relative;
    background: url('https://img.alicdn.com/imgextra/i2/732450928/TB22yXCbipnpuFjSZFIXXXh2VXa_!!732450928.png') no-repeat center;
    background-size: 100%;
    border-radius: 5px;
    box-shadow: 0 3px 4px #c9c9c9;
}

.sound-playTime {
    display: flex;
    align-items: center;
    font-size: 12px;
    line-height: 20px;
    width: 460rpx;
    position: absolute;
    bottom: 0;
    margin: 0 auto;
    left: 0;
    right: 0;
    justify-content: flex-end;
}

.icon-sound-playTimes {
    width: 20px;
    height: 20px;
    background: url('data:image/png;base64,') no-repeat center;
    background-size: 100%;
    margin-right: 2px;
    vertical-align: middle;
}

.text-playTimes {
    font-family: PingFangSC-Regular;
    font-size: 26rpx;
    color: #fff;
}

.process-panel {
    overflow: visible;
    padding: 14px 20px 29px;
    border-bottom: 10px solid #f3f4f6;
}

.time {
    display: flex;
    justify-content: space-between;
    line-height: 1;
}

.time-left,.time-right {
    font-size: 12px;
    color: #f86442;
}

.player-slider {
    position: relative;
    width: 100%;
    height: 20rpx;
    background: transparent;
    padding-top: 20rpx;
}

.player-inner {
    position: relative;
    height: 2rpx;
    background: #ddd;
    overflow: visible;
}

.player-inner::before {
    content: '';
    position: absolute;
    width: 3px;
    height: 3px;
    border-radius: 50%;
    background: #f86442;
    top: -1px;
    left: -1px;
}

.player-inner::after {
    content: '';
    position: absolute;
    top: -1px;
    right: -1px;
    width: 3px;
    height: 3px;
    border-radius: 50%;
    background: #ccc;
}

.player-load {
    height: 2rpx;
    background: #b6b6b6;
}

.player-play {
    position: absolute;
    top: 0;
    left: 0;
    max-width: 100%;
    height: 2rpx;
    background: #f86442;
    overflow: visible;
}

.player-dot {
    position: absolute;
    top: calc(2rpx - 70rpx /2);
    right: -35rpx;
    width: 70rpx;
    height: 70rpx;
    overflow: visible;
    display: flex;
    align-items: center;
    justify-content: center;
    z-index: 9;
}

.player-dot.on-hold::before {
    transform: scale(1.6,1.6);
}

.player-dot::before {
    content: '';
    width: 24rpx;
    height: 24rpx;
    background: #f86442;
    border-radius: 50%;
    box-shadow: 0 1px 3px 0 rgba(248,100,66,0.50);
    transition: all 0.2s;
}

.player-ctrl {
    display: flex;
    justify-content: space-around;
    align-items: center;
    margin-top: -24rpx;
}

.list-wrap {
    font-size: 9px;
    line-height: 13px;
    color: #bbb;
    text-align: center;
}

.icon-sound-list {
    width: 52rpx;
    height: 44rpx;
    display: block;
    background: url("data:image/png;base64,");
    background-size: 100%;
    margin: 20rpx auto 0;
}

.ctrl-center {
    display: flex;
    justify-content: center;
    align-items: center;
    flex: 1;
}

.icon-prev {
    width: 40rpx;
    height: 44rpx;
    background: url("data:image/png;base64,");
    background-size: 100%;
    background-repeat: no-repeat;
}

.icon-prev:active {
    transform: scale3d(1.3,1.3,1.3);
}

.icon-sound-play,.icon-sound-pause {
    width: 160rpx;
    height: 160rpx;
    background-size: 100%;
    margin: 0 64rpx;
    background-repeat: no-repeat;
}

.icon-sound-play {
    background-image: url("data:image/png;base64,");
}

.icon-sound-pause {
    background-image: url("data:image/png;base64,");
}

.icon-next {
    width: 40rpx;
    height: 44rpx;
    background: url("data:image/png;base64,");
    background-size: 100%;
    background-repeat: no-repeat;
}

.icon-next:active {
    transform: scale3d(1.3,1.3,1.3);
}

.icon-sound-history {
    width: 80rpx;
    height: 80rpx;
    background: url('https://img.alicdn.com/imgextra/i3/732450928/TB2mxAKcB0kpuFjy1XaXXaFkVXa_!!732450928.png') no-repeat;
    background-size: 100%;
}

.sound-brief {
    padding: 8px;
    border-bottom: 10px solid #f3f4f6;
}

.brief-title {
    display: block;
    font-family: PingFangSC-Regular;
    font-size: 30rpx;
    color: #000;
    margin-bottom: 30rpx;
}

.brief-cont {
    position: relative;
    font-family: ArialMT;
    font-size: 30rpx;
    color: #999;
    line-height: 50rpx;
}

.btn-pay {
    display: flex;
    justify-content: center;
}

.text-pay {
    display: inline-block;
    font-family: PingFangSC-Light;
    font-size: 26rpx;
    color: #f86442;
    padding: 16rpx 32rpx;
    border: 1rpx solid #f86442;
    border-radius: 4px;
}

.player-pauseBtn {
    display: none;
}

.player-is-paused .player-pauseBtn {
    display: none;
}

.player-is-paused .player-playBtn {
    display: block;
}

.player-is-playing .player-pauseBtn {
    display: block;
}

.player-is-playing .player-playBtn {
    display: none;
}

.inStart {
    opacity: 0.5;
}

.inEnd {
    opacity: 0.5;
}

.play-status-wrap {
    padding: 8rpx 42rpx;
    background: #fef7eb;
    font-size: 24rpx;
    color: #f5a623;
    letter-spacing: 0;
    margin: 0 -42rpx 25rpx;
    line-height: 1;
}

.to-buy {
    float: right;
    padding: 4rpx 12rpx;
    border: 2rpx solid #f5a623;
}

.play-status-tip {
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    line-height: 36rpx;
}

.pop-up-box-container {
    position: fixed;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    text-align: center;
    z-index: 999;
}

.pop-up-box-container::after {
    content: '';
    position: absolute;
    left: 0;
    right: 0;
    top: 0;
    bottom: 0;
    background: rgba(0,0,0,0.5);
}

.pop-up-box-container::before {
    content: '';
    display: inline-block;
    width: 0;
    height: 100%;
    vertical-align: middle;
}

.pop-up-box-wrap {
    box-sizing: border-box;
    position: relative;
    display: inline-block;
    width: 630rpx;
    vertical-align: middle;
    background: #fff;
    z-index: 1;
    padding: 56rpx;
    border-radius: 10rpx;
}

.box-hd text {
    display: block;
    font-size: 36rpx;
    font-weight: bold;
    margin: 36rpx auto 60rpx;
    line-height: 50rpx;
}

.box-bd {
    text-align: left;
    font-size: 32rpx;
    line-height: 32rpx;
    padding-bottom: 10rpx;
}

.box-bd-item {
    margin-bottom: 40rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.box-bd-item text {
    color: #fc5832;
}

.btn-to-pay {
    border-radius: 4rpx;
    color: #fff;
    font-size: 32rpx;
