.title {
    width: 100%;
    text-align: center;
    position: fixed;
    padding-top: 110rpx;
    top: 0;
    z-index: 9;
    background: #ffffff;
    height: 140rpx;
}

.title text {
    font-size: 42rpx;
    color: #4c4c4c;
}

.flex {
    display: flex;
    flex-direction: column;
    justify-content: center;
    font-size: 32rpx;
}

.kcbtn {
    width: 90%;
    text-align: center;
    border-radius: 10rpx;
    margin: 0 auto 28rpx auto;
    height: 80rpx;
    line-height: 80rpx;
    font-size: 32rpx;
}

.kcbtn1 {
    background: #f5f5f5;
    color: #666666;
}

.kemu {
    height: 882rpx;
    position: relative;
    padding-top: 28rpx;
    margin-top: 255rpx;
}

.btm-icon {
    width: 97%;
    height: 120rpx;
    background-image: url('http://img02.exam8.com/img2017/minapp/icon/xk-icon.png');
    background-size: 100%;
    position: fixed;
    bottom: 0;
    background-position: center bottom;
    background-repeat: no-repeat;
}

.top-icon {
    width: 97%;
    height: 60rpx;
    background-image: url('http://img02.exam8.com/img2017/minapp/icon/xk-icon1.png');
    background-size: 100%;
    position: fixed;
    top: 235rpx;
    background-position: center bottom;
    background-repeat: no-repeat;
}

