.flex-start {
    display: flex;
    justify-content: flex-start;
    align-items: center;
}

.flex-center {
    display: flex;
    justify-content: center;
    align-items: center;
}

.flex-shrink-no {
    flex-shrink: 0;
}

.word-wrap {
    word-wrap: break-word;
    white-space: pre-line;
}

.analysis-box {
    margin: 0 30rpx;
    margin-top: 20rpx;
    box-sizing: border-box;
    background-color: #fff;
    padding: 0 30rpx;
    border-radius: 10rpx;
}

.analysis-title {
    color: #3e4855;
    font-size: 32rpx;
    padding: 30rpx 0;
    border-bottom: 1px solid #eef0f3;
}

.analysis-content {
    padding: 20rpx 0;
    font-size: 28rpx;
    color: #3e4855;
}

.analysis-content .title {
    font-size: 32rpx;
    margin-bottom: 20rpx;
}

.list-empty {
    background-color: #fff;
    padding: 150rpx 0 200rpx;
    margin: 0 30rpx;
    border-radius: 10rpx;
}

.list-empty .empty-box {
    background-image: url('http://static3topen.jingdaka.com/images/wrongQuestionEmpty.png');
    background-repeat: no-repeat;
    background-size: 100% auto;
    height: 340rpx;
    width: 100%;
    position: relative;
    text-align: center;
}

.list-empty .tips {
    width: 100%;
    text-align: center;
    margin-top: -50rpx;
    color: #8d97a4;
    font-size: 32rpx;
}