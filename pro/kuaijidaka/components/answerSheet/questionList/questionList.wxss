.border-box {
    box-sizing: border-box;
}

.active {
    color: #22dd82;
    border-top: 5rpx solid #22dd82;
    transition: all 0.5s;
}

.noActive {
    color: #9fa9b4;
    border-top: 5rpx solid rgba(0,0,0,0);
}

.line-bg {
    width: 100%;
    height: 60rpx;
    border-top: 1rpx solid #ecf0f3;
}

.por {
    position: relative;
