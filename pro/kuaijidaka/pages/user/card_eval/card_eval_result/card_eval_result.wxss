page {
    padding-bottom: 60rpx;
}

.content_bg {
    padding: 0 30rpx;
    box-sizing: border-box;
}

.score_bg {
    background-color: #fff;
    border-radius: 8rpx 8rpx 0 0;
    display: flex;
    justify-content: center;
    align-items: center;
    flex-direction: column;
    text-align: center;
}

.words {
    color: #22dd82;
    font-size: 32rpx;
    margin-top: 400rpx;
}

.score_number .words {
    margin-top: 220rpx;
}

.words--no-eval {
    color: rgb(85,94,105);
    font-size: 32rpx;
    margin-bottom: 30rpx;
}

.qs_content_bg {
    background-color: #fff;
    border-radius: 8rpx;
}

