    color: #3e4855;
}

.col8 {
    color: #8b97a4;
}

.colb4bcc6 {
    color: #b4bcc6;
}

.score_no_eval {
    width: 249rpx;
    height: 211rpx;
    background: url('http://static3topen.jingdaka.com/images/sprites.png?v=41');
    background-repeat: no-repeat;
    background-size: 300rpx;
    background-position: -30rpx -1388rpx;
    margin-top: 20rpx;
}

.my_score {
    color: #fff;
    margin-top: 75rpx;
    font-size: 36rpx;
}

.score {
    color: #fff;
    position: absolute;
    top: 120rpx;
    left: 115rpx;
}

.score_number {
    width: 260rpx;
    height: 211rpx;
    background: url('http://static3topen.jingdaka.com/images/sprites.png?v=41');
    background-repeat: no-repeat;
    background-size: 300rpx;
    background-position: -30rpx -1388rpx;
    margin: 20rpx 0 70rpx;
    position: relative;
    text-align: center;
}

.score_list--number {
    flex-direction: row;
}

.item-text {
    margin: 0 23rpx;
}

.item-score {
    font-size: 24rpx;
    color: #fed814;
    text-align: center;
    line-height: 40rpx;
    position: absolute;
    left: 50%;
    bottom: -20rpx;
    margin-left: -38rpx;
}

.ml5 {
    margin-left: 5rpx;
}

.submit-container {
    width: 100%;
    background: #fff;
    border-radius: 0 0 8rpx 8rpx;
    box-sizing: border-box;
    font-size: 22rpx;
    color: #8b97a4;
}

.border-top {
    height: 1rpx;
    width: 638rpx;
    margin: 0 auto 15rpx;
    background: #edf0f3;
}