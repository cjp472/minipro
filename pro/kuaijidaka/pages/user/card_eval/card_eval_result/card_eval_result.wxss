.score_content {
    padding-bottom: 5rpx;
    display: flex;
    justify-content: flex-start;
    align-items: center;
    flex-wrap: wrap;
}

.score_list {
    width: 135rpx;
    height: 68rpx;
    margin-left: 30rpx;
    margin-bottom: 35rpx;
    position: relative;
    background-color: #f8fafb;
    border-radius: 4rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    flex-direction: column;
}

.score_icon {
    width: 160rpx;
    height: 100%;
    position: absolute;
    left: 50%;
    bottom: -25rpx;
    margin-left: -80rpx;
    box-sizing: border-box;
    display: flex;
    justify-content: space-between;
    align-items: flex-end;
    transform: scale(0.7);
}

.star_score {
    width: 50rpx;
    height: 50rpx;
    background-position: -134rpx -990rpx;
}

.star_score2 {
    width: 50rpx;
    height: 50rpx;
    background-position: -200rpx -990rpx;
}

.no_eval_data {
    height: 239rpx;
    background-color: #fff;
    border-radius: 4px;
    font-size: 28rpx;
    color: #b4bcc6;
    text-align: center;
    line-height: 239rpx;
}

button::after {
    border: none;
}

.wx-button2 {
    margin-right: 30rpx;
    border: 1rpx solid #e3e8ee;
}

.wx-button3 {
    border: 1rpx solid #22dd82;
}

