page {
    background: #f5f5f5;
}

.container {
    background: #f5f5f5;
}

.bg-white {
    display: block;
    background: #ffffff;
}

.bg-img {
    display: block;
    width: 100%;
    height: 584rpx;
}

.bg-des {
    text-align: center;
    font-size: 30rpx;
    color: #7d829a;
    padding-bottom: 48rpx;
}

.bg-go {
    width: 200rpx;
    height: 80rpx;
    line-height: 80rpx;
    border-radius: 40rpx;
    text-align: center;
    margin: 0 auto;
    font-size: 34rpx;
    color: #fff;
    background-color: #ef533d;
}

.show {
    background: #f5f5f5;
}

.list {
    background: #fff;
    padding: 0 32rpx 27rpx 31rpx;
    margin-bottom: 10rpx;
    position: relative;
    border-bottom: 10rpx solid #f5f5f5;
}

.list .title {
    padding-top: 23rpx;
    font-size: 28rpx;
    font-weight: 700;
    color: #2a2e3d;
    line-height: 42rpx;
}

.time-range {
    color: #7d829a;
    line-height: 44rpx;
    font-size: 23rpx;
    position: relative;
    padding-left: 29rpx;
}

.teacher {
    color: #7d829a;
    line-height: 36rpx;
    font-size: 22rpx;
    padding-bottom: 22rpx;
}

.time-class {
    color: #2a2e3d;
    font-size: 22rpx;
    line-height: 22rpx;
}

.time-icon {
    width: 18rpx;
    height: 18rpx;
    position: absolute;
    top: 0;
    left: 0;
}

.time-icon image {
    width: 100%;
    height: 100%;
}

.info-btn {
    position: absolute;
    bottom: 30rpx;
    right: 32rpx;
    font-size: 24rpx;
    line-height: 42rpx;
    padding: 0 19rpx;
    border: 1px solid #f26e5b;
    border-radius: 22rpx;
}

.already-on {
    color: #f99a4a;
    border: 1px solid #f99a4a;
}

.begin {
    color: #f26e5b;
    border: 1px solid #f26e5b;
}

.over-class .teacher {
    padding-top: 8rpx;
    padding-bottom: 0rpx;
}

.over-class .list {
    padding-bottom: 22rpx;
}

.over-class .title {
}

.tab-line {
    display: inline-block;
    width: 30rpx;
    height: 6rpx;
    margin: 19rpx auto 0;
    border-radius: 3rpx;
    background: #fff;
}

.bold {
    font-weight: bold;
}