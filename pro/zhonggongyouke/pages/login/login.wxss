page {
    background-color: #f5f5f5;
}

.container {
    display: block;
    height: 100%;
    padding-bottom: 30rpx;
    background-color: #f5f5f5;
}

.logo {
    display: block;
    width: 180rpx;
    height: 180rpx;
    margin: 200rpx auto 0;
}

.title {
    width: 100%;
    height: 50rpx;
    margin-bottom: 37rpx;
    line-height: 50rpx;
    text-align: center;
    font-size: 28rpx;
    color: #7d829a;
}

.btn-t {
    width: 590rpx;
    height: 88rpx;
    margin-bottom: 40rpx;
    line-height: 88rpx;
    font-size: 32rpx;
    color: #fff;
}

.btn-b {
    box-sizing: border-box;
    width: 588rpx;
    height: 88rpx;
    line-height: 88rpx;
    border: 2rpx solid #7d829a;
    margin: 0 auto;
    background: #f5f5f5;
    font-size: 32rpx;
    color: #7d829a;
    border-radius: 10rpx;
    text-align: center;
}