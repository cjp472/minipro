@import "../publiclist/publiclist.wxss";

page {
    background: #ffffff;
}

.public-title {
    padding: 20rpx 30rpx 30rpx 30rpx;
    border-bottom: 10rpx solid #f5f5f5;
    background: #fff;
}

.public-title-head {
    line-height: 46rpx;
    font-size: 32rpx;
    color: #2a2e3d;
}

.public-title-bottom {
    box-sizing: border-box;
    height: 33rpx;
    padding-top: 11rpx;
    line-height: 22rpx;
    font-size: 22rpx;
    color: #7d829a;
}

.public-title-bottom text {
    margin-right: 8rpx;
}

.public-learns {
    display: inline-block;
    float: right;
}